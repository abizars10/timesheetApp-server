{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/kegiatan", "dest": "index.js" },
    { "src": "/karyawan", "dest": "index.js" },
    { "src": "/proyek", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
