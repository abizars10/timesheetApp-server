{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/kegiatan", "dest": "index.js" },
    { "src": "/api/karyawan", "dest": "index.js" },
    { "src": "/api/proyek", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
